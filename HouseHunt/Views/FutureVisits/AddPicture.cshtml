@model Picture
@{
    ViewData["Title"] = "AddPicture";
}

<h1>AddPicture</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="AddPicture">
            <input type="hidden" asp-for="HouseId" />
            <div class="form-group">
                <label asp-for="URL" class="control-label"></label>
                <input asp-for="URL" class="form-control" />
                <span asp-validation-for="URL" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Add Picture" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>